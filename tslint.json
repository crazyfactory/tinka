{
    "rules": {
        "class-name": true,
        "max-line-length": [
            true,
            140
        ],
        "no-arg": true,
        "no-bitwise": true,
        "no-string-literal": false,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "curly": true,
        "eofline": true,
        "forin": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "indent": [
            true,
            "spaces"
        ],
        "label-position": true,
        "label-undefined": true,
        "no-duplicate-variable": true,
        "no-debugger": true,
        "no-duplicate-key": true,
        "no-eval": true,
        "no-empty": true,
        "no-internal-module": true,
        "no-trailing-whitespace": true,
        "trailing-comma": false,
        "no-unsafe-finally": true,
        "no-unused-variable": false,
        "no-unreachable": true,
        "no-use-before-declare": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "no-var-keyword": true,
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    }
}